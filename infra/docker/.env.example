# AgenticLab Docker Environment Variables
# Copy this to .env and fill in your values:
#   cp .env.example .env

# HuggingFace token — needed for gated models (Llama 3.3, etc.)
# Get yours at: https://huggingface.co/settings/tokens
HF_TOKEN=

# vLLM API key — used for OpenAI-compatible endpoint auth
VLLM_API_KEY=agenticlab-dev
